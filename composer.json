{
  "name": "arraypress/wp-asset-loader",
  "version": "1.0.0",
  "description": "A simple utility for loading WordPress assets from Composer libraries.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-asset-loader/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\WP\\": "src/"
    },
    "files": [
      "src/Utilities.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-asset-loader/issues",
    "source": "https://github.com/arraypress/wp-asset-loader"
  },
  "keywords": [
    "wordpress",
    "assets",
    "composer",
    "library",
    "css",
    "javascript",
    "enqueue",
    "wp-enqueue"
  ]
}